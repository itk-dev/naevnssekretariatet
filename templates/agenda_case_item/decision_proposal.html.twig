{% extends 'layout-with-agenda-case-item-submenu.html.twig' %}

{% trans_default_domain 'agenda_item' %}

{% block title %}{% trans %}Case decision proposal{% endtrans %}{% endblock %}

{% block header %}
    <div class="row">
        <div class="col">
            <h1>{% trans %}Case decision proposal{% endtrans %}</h1>
            <p class="text-muted"><small>{% trans %}Create or update case decision proposal{% endtrans %}</small></p>
        </div>
    </div>
{% endblock %}

{% block content %}

    <main role="main" class="col-12 mt-3">
        {{ form(decision_proposal_form) }}
    </main>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}